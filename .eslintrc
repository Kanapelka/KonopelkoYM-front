{
  "extends": ["airbnb"],
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "max-len": ["error", { "code": 120 }],
    "linebreak-style": "off",
    "import/no-unresolved": 0,
    "react/jsx-filename-extension": 0,
    "import/extensions": 0,
    "@typescript-eslint/no-unused-vars": [2, {"args": "none"}]
  },
   "env": {
     "browser": true,
     "node": false
   }
}
